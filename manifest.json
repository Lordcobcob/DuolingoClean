{
  "name": "Duolingo Clean Themes",
  "version": "1.2",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": ["https://www.duolingo.com/*"],
      "resources": [ "CustomLingotDesign.svg" ],
      "css": ["style.css"],
      "js": ["javascript.js"]
    },
	{
      "matches": ["https://preview.duolingo.com/*"],
      "resources": [ "CustomLingotDesign.svg" ],
      "css": ["style.css"],
      "js": ["javascript.js"]
    }
  ]
}
